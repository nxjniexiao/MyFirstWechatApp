/* pages/welcome/welcome.wxss */
page{
  height: 100%;
  background-color: #b3d4db;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.user-avatar{
  width: 200rpx;
  height: 200rpx;
  margin-top: 160rpx;
  margin-bottom: 50rpx;
}
.user-text{
  font-size: 32rpx;
  font-weight: bold;
}
.moto-container{
  margin-top: 200rpx;
  width: 200rpx;
  height: 80rpx;
  border: 1px solid #405f80;
  border-radius: 5px;
  text-align: center;
}
.moto{
  line-height: 80rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: #405f80;
}